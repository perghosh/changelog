      </FIELD>
      <FIELD NAME="StateC" ALIAS="State" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="vote.poll.state">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11021 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="FName" ALIAS="Name" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FHeader" ALIAS="Header" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FDescription" ALIAS="Description" TYPE="STR" CONDITION="1" />
      <FIELD ID="Note" NAME="FDescription" ALIAS="Note" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _n.FNote FROM $O{}TNote _n WHERE _n.RecordK=$T.PollK AND _n.table_number=11020)</EXPRESSION>
      </FIELD>
      <FIELD NAME="FBegin" ALIAS="Begin" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="FBegin" SIMPLENAME="~TPollFBegin FBegin" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="FEnd" ALIAS="End" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="FEnd" SIMPLENAME="~TPollFEnd FEnd" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="FRate" ALIAS="Rate" TYPE="R8" CONDITION="1" />
      <FIELD NAME="FCount" ALIAS="Count" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FDelay" ALIAS="Delay" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FDelayVote" ALIAS="DelayVote" TYPE="R8" CONDITION="1" />
      <FIELD NAME="FWeight" ALIAS="Weight" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FUseTie" ALIAS="Tie" TYPE="I2" CONDITION="1" />
      <FIELD ID="UseTieName" NAME="FUseTie" ALIAS="Use tie" TYPE="I2" CONDITION="1">
         <EXPRESSION>CASE WHEN $T.$F = 0 THEN 'NO' ELSE 'YES' END</EXPRESSION>
         <LOOKUP CACHE="yes_no" />
      </FIELD>
      <FIELD NAME="FDeleted" ALIAS="Deleted" TYPE="I2" CONDITION="1" />
      <FIELD ID="CountQuestion" NAME="PollK" ALIAS="Count questions" SIMPLENAME="Number of questions" TYPE="INT" CONDITION="expression" DESCRIPTION="Count number questions for poll">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{VOTE}TPollQuestion _pq WHERE _pq.PollK = $T.PollK)</EXPRESSION>
      </FIELD>
      <FIELD ID="CountLink" NAME="PollK" ALIAS="Count link" SIMPLENAME="Number of links" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{}TLink _l WHERE _l.ParentK=$T.PollK AND _l.table_number=11020)</EXPRESSION>
      </FIELD>
      <FIELD ID="CountCommentQuestion" NAME="PollK" ALIAS="Count link" SIMPLENAME="Number of links" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{VOTE}TPollLimit _pl JOIN $O{VOTE}TPollQuestion _pq ON _pl.PollQuestionK=_pq.PollQuestionK WHERE _pq.PollK=$T.PollK AND _pl.limit_type=9 AND _pl.FLimitInteger=1) /*9 = comment*/</EXPRESSION>
      </FIELD>
      <FIELD ID="CountPoll" NAME="PollK" ALIAS="Count poll" SIMPLENAME="Count connected poll" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{VOTE}TrPollXPoll x WHERE x.PollK=$T.PollK)</EXPRESSION>
      </FIELD>
      <FIELD ID="CountMyVoterCount" NAME="PollK" ALIAS="Count my votes" SIMPLENAME="Number of links" TYPE="INT" CONDITION="expression">
         <EXPRESSION>
(SELECT COUNT(*) FROM $O{VOTE}TPollVote _pv JOIN $O{VOTE}TPollQuestion _pq ON _pv.PollQuestionK=_pq.PollQuestionK
WHERE _pq.PollK=$T.PollK AND _pv.VoterK=${{__user.get_value(...2, &quot;@@voter&quot;, &quot;-1&quot;)}})
         </EXPRESSION>
      </FIELD>
      <FIELD ID="CountIpVoterCount" NAME="PollK" ALIAS="Count ip votes" SIMPLENAME="Number of user ip votes" TYPE="INT" CONDITION="expression">
         <EXPRESSION>
(SELECT COUNT(*) FROM $O{VOTE}TPollVote _pv JOIN $O{VOTE}TPollQuestion _pq ON _pv.PollQuestionK=_pq.PollQuestionK
WHERE _pq.PollK=$T.PollK AND _pv.FIp='${{__user.get_value(...2, &quot;@@ip&quot;, &quot;&quot;)}}')
         </EXPRESSION>
      </FIELD>
      
      <CONDITION ID="Hashtag" NAME="PollK" SIMPLENAME="Hashtag" TYPE="STR" NOVALUE="1" INTELLISENSE="hashtag_name">
         <EXPRESSION>
$T.$F $_operator (SELECT _r.ParentK FROM $OTBadge _b JOIN $OTrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _b.FName $_condition AND _r.table_number=11020)
         </EXPRESSION>
         <OPERATORS>
            <OPERATOR FROM="0" TO="11" />
            <OPERATOR FROM="5" TO="12" />
         </OPERATORS>
      </CONDITION>
      
      <CONDITION ID="HashtagKey" NAME="PollK" SIMPLENAME="Hashtag" TYPE="INT" NOVALUE="1">
         <EXPRESSION>
$T.$F $_operator (SELECT _r.ParentK FROM $O{}TBadge _b JOIN $O{}TrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _b.BadgeK $_condition AND _r.table_number=11020)
         </EXPRESSION>
         <OPERATORS>
            <OPERATOR FROM="0" TO="11" />
            <OPERATOR FROM="5" TO="12" />
         </OPERATORS>
      </CONDITION>
      
      <EDIT NAME="PollGroupK" ALIAS="Group" TYPE="I4" />
      <EDIT NAME="ParentK" ALIAS="ParentK" TYPE="I4" />
      <EDIT NAME="table_number" ALIAS="table_number" TYPE="I4" />
      <EDIT NAME="SuperK" ALIAS="SuperK" TYPE="I4" />
      <EDIT NAME="UserK" ALIAS="UserK" TYPE="I4" />
      <EDIT NAME="CreateD" ALIAS="CreateD" TYPE="DATE" />
      <EDIT NAME="UpdateD" ALIAS="UpdateD" TYPE="DATE" />
      <EDIT NAME="TypeC" ALIAS="Type" TYPE="I4">
         <LOOKUP CACHE="vote.poll.type.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11020 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="StateC" ALIAS="State" TYPE="I4">
         <LOOKUP CACHE="vote.poll.state.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11021 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="FName" ALIAS="Name" TYPE="STR" MAX="500" />
      <EDIT NAME="FHeader" ALIAS="Header" TYPE="STR" MAX="200" />
      <EDIT NAME="FDescription" ALIAS="Description" TYPE="STR" />
      <EDIT NAME="FBegin" ALIAS="Begin" TYPE="DATE" />
      <EDIT NAME="FEnd" ALIAS="End" TYPE="DATE" />
      <EDIT NAME="FRate" ALIAS="Rate" TYPE="R8" />
      <EDIT NAME="FCount" ALIAS="Count" TYPE="I4" />
      <EDIT NAME="FDelay" ALIAS="Delay" TYPE="I4" />
      <EDIT NAME="FDelayVote" ALIAS="DelayVote" TYPE="R8" />
      <EDIT NAME="FUseTie" ALIAS="Use tie" TYPE="I2">
         <LOOKUP CACHE="yes_no" />
      </EDIT>
      <EDIT NAME="FWeight" ALIAS="Weight" TYPE="I4" />
      <EDIT NAME="FDeleted" ALIAS="Deleted" TYPE="I2" />
   </TABLETEMPLATE>
   
