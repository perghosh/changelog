   <TABLETEMPLATE ID="vote.pollgroup">
      <FIELD NAME="ParentK" ALIAS="Parent" TYPE="I4" CONDITION="1" />
      <FIELD NAME="table_number" ALIAS="table_number" TYPE="I4" CONDITION="1" />
      <FIELD NAME="SuperK" ALIAS="Super" TYPE="I4" CONDITION="1" />
      <FIELD ID="SuperK-Name" NAME="SuperK" ALIAS="Parent name" TYPE="I4" DISPLAYTYPE="STR">
         <EXPRESSION>(SELECT t.FName FROM $O{VOTE}PollGroup t WHERE t.PollGroupK=$T.SuperK)</EXPRESSION>
      </FIELD>
      <CONDITION ID="SuperK-Name" NAME="SuperK" SIMPLENAME="Parent system" TYPE="STR" NOMERGE="1" NOVALUE="1">
         <EXPRESSION>$T.SuperK IN (SELECT t.PollGroupK FROM $O{VOTE}TPollGroup t WHERE t.FName $_condition)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="UserK" ALIAS="User" TYPE="I4" CONDITION="1" />
      <FIELD NAME="CreateD" ALIAS="Create" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="CreateD" SIMPLENAME="~TPollGroupCreateD CreateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="UpdateD" ALIAS="Update" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="UpdateD" SIMPLENAME="~TPollGroupUpdateD UpdateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="TypeC" ALIAS="Type" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.TypeC)</EXPRESSION>
         <LOOKUP CACHE="pollgroup.type">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11010 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="StateC" ALIAS="State" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.StateC)</EXPRESSION>
         <LOOKUP CACHE="pollgroup.state">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11011 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="PriorityC" ALIAS="Priority" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.PriorityC)</EXPRESSION>
         <LOOKUP CACHE="pollgroup.priority">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11012 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="AreaC" ALIAS="Area" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.AreaC)</EXPRESSION>
         <LOOKUP CACHE="pollgroup.area">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11013 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="FName" ALIAS="Name" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FAbbreviation" ALIAS="Abbreviation" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FDescription" ALIAS="Description" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FUseTie" ALIAS="Use tie" TYPE="I2" CONDITION="1">
         <EXPRESSION>CASE WHEN $T.$F = 0 THEN 'NO' ELSE 'YES' END</EXPRESSION>
         <LOOKUP CACHE="yes_no" />
      </FIELD>
      <FIELD NAME="FIdle" ALIAS="Idle" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDeleted" ALIAS="Deleted" TYPE="I2" CONDITION="1" />
      
      <FIELD ID="FName-Indent" NAME="FName" ALIAS="Indented Name" CONDITION="1" TYPE="STR" FF="v">
         <FORMULA TYPE="HTML" />
         <EXPRESSION>
('&lt;span style="margin-left:' + CAST( ISNULL( (SELECT TOP 1 _t.FDepth FROM $O{}thread _t WHERE _t.FKey=$T.PollGroupK AND _t."table_number" = 11010), 0 ) AS VARCHAR(10)) + 'em;"&gt;' + $T.FName + '&lt;/span&gt;')
         </EXPRESSION>
         <EXPRESSION OUTPUT="edit">$T.FName</EXPRESSION>
      </FIELD>
      
      <EDIT NAME="ParentK" ALIAS="Parent" TYPE="I4" />
      <EDIT NAME="table_number" ALIAS="table_number" TYPE="I4" />
      <EDIT NAME="SuperK" ALIAS="Super" TYPE="I4" />
      <EDIT NAME="UserK" ALIAS="User" TYPE="I4" />
      <EDIT NAME="CreateD" ALIAS="CreateD" TYPE="DATE" />
      <EDIT NAME="UpdateD" ALIAS="UpdateD" TYPE="DATE" />
      <EDIT NAME="TypeC" ALIAS="Type" TYPE="I4">
         <LOOKUP CACHE="pollgroup.type.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11010 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="StateC" ALIAS="State" TYPE="I4">
         <LOOKUP CACHE="pollgroup.state.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11012 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="PriorityC" ALIAS="Priority" TYPE="I4">
         <LOOKUP CACHE="pollgroup.priority.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11012 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="AreaC" ALIAS="Area" TYPE="I4">
         <LOOKUP CACHE="pollgroup.priority.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=11013 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="FName" ALIAS="Name" TYPE="STR" MAX="100" />
      <EDIT ID="FName-Indent" NAME="FName" ALIAS="Name" TYPE="STR" MAX="100" />
      <EDIT NAME="FAbbreviation" ALIAS="Abbreviation" TYPE="STR" MAX="100" />
      <EDIT NAME="FDescription" ALIAS="Description" TYPE="STR" MAX="1000" />
      <EDIT NAME="FUseTie" ALIAS="Use tie" TYPE="I2">
         <LOOKUP CACHE="yes_no" />
      </EDIT>
      <EDIT NAME="FIdle" ALIAS="Idle" TYPE="I2" />
      <EDIT NAME="FDeleted" ALIAS="Deleted" TYPE="I2" />
   </TABLETEMPLATE>

   <TABLETEMPLATE ID="vote.poll">
      <FIELD ID="PollGroupK-Id" NAME="PollGroupK" ALIAS="Group" TYPE="INT" CONDITION="1" />
      <FIELD NAME="PollGroupK" ALIAS="Group" TYPE="INT" DISPLAYTYPE="STR" CONDITION="expression">
