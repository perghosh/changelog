   <INTELLISENSES>
      <INTELLISENSE ID="object_thread_name" TABLE="TObject1" SQL="SELECT TOP 20 _o.FName FROM $O{api}TObject _o JOIN $Othread_header _th ON _o.ObjectK = _th.FThreadId AND _th.&quot;Table&quot;=10010 WHERE _o.FName like '{{=this}}%' ORDER BY 1" MASK="V" DESCRIPTION="Thread name" />
   </INTELLISENSES>

   <TABLETEMPLATE ID="application.system">
      <FIELD ID="CountObject" NAME="SystemK" ALIAS="Count obj." SIMPLENAME="Number of objects" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{API}TObject _o JOIN $OTrSystemXApiObject _x ON _o.ObjectK=_x.ObjectK WHERE _x.SystemK = $T.SystemK AND _o.SuperK IS NULL)</EXPRESSION>
      </FIELD>
   </TABLETEMPLATE>


   <TABLETEMPLATE ID="api.object">
      <FIELD NAME="RepositoryK" ALIAS="Repository" TYPE="I4" CONDITION="1">
         <EXPRESSION>(SELECT _r.FName FROM $O{APP}TRepository _r WHERE _r.RepositoryK=$T.RepositoryK)</EXPRESSION>
      </FIELD>
      
      <FIELD NAME="SuperK" ALIAS="Super" TYPE="I4" CONDITION="1">
         <EXPRESSION>(SELECT COALESCE(_o.FName,_o.FDeclaration,'') FROM $O{APP}TObject _o WHERE _o.ObjectK=$T.SuperK)</EXPRESSION>
      </FIELD>
      <FIELD NAME="CreateD" ALIAS="Create" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="CreateD" SIMPLENAME="~TObjectCreateD CreateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="UpdateD" ALIAS="Update" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="UpdateD" SIMPLENAME="~TObjectUpdateD UpdateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="EnvironmentC" ALIAS="Environment" TYPE="I4" DISPLAYTYPE="STR"  CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.environment">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10010 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="StateC" ALIAS="State" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.state">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10011 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="TypeC" ALIAS="Type" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.type">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10012 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="NamespaceC" ALIAS="Namespace" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.namespace">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10013 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="LanguageC" ALIAS="Language" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.language">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10014 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="ReturnC" ALIAS="return" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.language">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10015 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="FName" ALIAS="Name" TYPE="STR" CONDITION="1" />
      <FIELD ID="FName-Indent" NAME="FName" ALIAS="Indented name" CONDITION="1" TYPE="STR" DESCRIPTION="Indent name based on level in object tree if it belongs to any">
         <FORMULA TYPE="HTML" />
         <EXPRESSION>
('&lt;span style="margin-left:' + CAST( ISNULL( (SELECT TOP 1 _t.FDepth FROM $O{}thread _t WHERE _t.FKey=$T.ObjectK AND _t."table_number" = 10010), 0 ) * 2 AS VARCHAR(10)) + 'em;"&gt;' + $T.FName + '&lt;/span&gt;')
         </EXPRESSION>
      </FIELD>
      <FIELD NAME="FInternalName" ALIAS="Internal name" TYPE="STR" CONDITION="1" />      
      <FIELD ID="Note" NAME="FDescription" ALIAS="Note" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _n.FNote FROM $O{}TNote _n WHERE _n.RecordK=$T.ObjectK AND _n.table_number=10010)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="CountBook" NAME="ObjectK" ALIAS="Count book" SIMPLENAME="Number of books" TYPE="INT" CONDITION="expression" DESCRIPTION="Count number of books connected to object">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTrArticleBookXApiObject _r WHERE _r.ObjectK = $T.ObjectK)</EXPRESSION>
      </FIELD>
      
      <FIELD NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FDescription" ALIAS="Description" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FVersionBegin" ALIAS="VersionBegin" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FVersionEnd" ALIAS="VersionEnd" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FBeta" ALIAS="Beta" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDeleted" ALIAS="Deleted" TYPE="I2" CONDITION="1" />

      <FIELD ID="CountBooks" NAME="ObjectK" ALIAS="Count link" SIMPLENAME="Number of links" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{}TrArticleBookXApiObject _x WHERE _x.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      
      
      <FIELD ID="CountExample" NAME="ObjectK" ALIAS="Count example" SIMPLENAME="Number of examples" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTExample _e WHERE _e.ParentK=$T.ObjectK AND _e.table_number = 10010)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="CountFunction" NAME="ObjectK" ALIAS="Count function" SIMPLENAME="Number of functions" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTFunction _f WHERE _f.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="CountParameter" NAME="ObjectK" ALIAS="Count parameter" SIMPLENAME="Number of parameters" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTParameter _p WHERE _p.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="CountLink" NAME="ObjectK" ALIAS="Count link" SIMPLENAME="Number of links" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{}TLink _l WHERE _l.ParentK=$T.ObjectK AND _l.table_number=10010)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="Hashtag" NAME="ObjectK" ALIAS="Hashtag" TYPE="STR" DESCRIPTION="Hastags connected to object">
         <EXPRESSION>
CAST( 
STUFF( 
   (SELECT ',' + _b.FName FROM $OTBadge _b JOIN $OTrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _r.ParentK=$T.ObjectK AND _r.table_number=10010),
   1,1,'' )
   AS NVARCHAR(512)
)
         </EXPRESSION>
      </FIELD>
      <CONDITION ID="Hashtag" NAME="ObjectK" SIMPLENAME="Hashtag" TYPE="STR" NOVALUE="1" INTELLISENSE="hashtag_name">
         <EXPRESSION>
$T.$F $_operator (SELECT _r.ParentK FROM $O{}TBadge _b JOIN $O{}TrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _b.FName $_condition AND _r.table_number=10010)
         </EXPRESSION>
         <OPERATORS>
            <OPERATOR FROM="0" TO="11" />
            <OPERATOR FROM="5" TO="12" />
         </OPERATORS>
      </CONDITION>
      
      <FIELD ID="ThreadNumber" NAME="ObjectK" ALIAS="Thread ID" TYPE="INT" CONDITION="expression" DESCRIPTION="The thread number, same as key to system root">
         <EXPRESSION>(SELECT TOP 1 _th.FThreadId FROM $O{}thread_header _th JOIN $O{}thread _t ON _th.FThreadId = _t.FThreadId WHERE _t.FKey=$T.ObjectK AND _t."table_number" = 10010)</EXPRESSION>
      </FIELD>
      <FIELD ID="ThreadName" NAME="ObjectK" ALIAS="Thread Name" TYPE="STR" CONDITION="expression" INTELLISENSE="object_thread_name" DESCRIPTION="The thread name, same as name to root object">
         <EXPRESSION>(SELECT TOP 1 _o.FName FROM $OTObject _o JOIN $O{}thread_header _th ON _o.ObjectK = _th.FThreadId AND _th."table_number" = 10010 JOIN $Othread _t ON _th.FThreadId = _t.FThreadId AND _t."table_number" = 10010 WHERE _t.FKey=$T.ObjectK)</EXPRESSION>
      </FIELD>
      
      
      <EDIT NAME="CreateD" ALIAS="Create" TYPE="DATE" />
      <EDIT NAME="UpdateD" ALIAS="Update" TYPE="DATE" />
      <EDIT NAME="EnvironmentC" ALIAS="Environment" TYPE="I4">
         <LOOKUP CACHE="api.object.environment.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10010 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="StateC" ALIAS="State" TYPE="I4">
         <LOOKUP CACHE="api.object.state.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10011 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="TypeC" ALIAS="Type" TYPE="I4">
         <LOOKUP CACHE="api.object.type.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10012 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="NamespaceC" ALIAS="Namespace" TYPE="I4">
         <LOOKUP CACHE="api.object.namespace.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10013 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="LanguageC" ALIAS="Language" TYPE="I4">
         <LOOKUP CACHE="api.object.language.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10014 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="ReturnC" ALIAS="Return" TYPE="I4">
         <LOOKUP CACHE="api.object.return.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10015 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="FName" ALIAS="Name" TYPE="STR" MAX="300" />
      <EDIT NAME="FInternalName" ALIAS="Name" TYPE="STR" MAX="100" />
      <EDIT NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" MAX="1000" />
      <EDIT NAME="FDescription" ALIAS="Description" TYPE="STR" MAX="1000" />
      <EDIT NAME="FVersionBegin" ALIAS="VersionBegin" TYPE="I4" />
      <EDIT NAME="FVersionEnd" ALIAS="VersionEnd" TYPE="I4" />
      <EDIT NAME="FBeta" ALIAS="Beta" TYPE="I2" />
      <EDIT NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" />
      <EDIT NAME="FDeleted" ALIAS="Deleted" TYPE="I2" />
   </TABLETEMPLATE>
   
