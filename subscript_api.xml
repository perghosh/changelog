   <TABLETEMPLATE ID="api.parameter">
      <FIELD NAME="FunctionK" ALIAS="Function" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _o.FName FROM $OTObject _o WHERE _o.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      <FIELD NAME="ObjectK" ALIAS="Object" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _o.FName FROM $OTObject _o WHERE _o.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      <FIELD ID="FunctionOrObject" ALIAS="Object owner" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(CASE WHEN $T.ObjectK IS NULL THEN (SELECT t.FName FROM $OTFunction t WHERE t.FunctionK=$T.FunctionK) ELSE (SELECT t.FName FROM $OTObject t WHERE t.ObjectK=$T.ObjectK) END)</EXPRESSION>
      </FIELD>
      <FIELD NAME="ParameterTypeK" ALIAS="Parameter type" TYPE="I4" CONDITION="1" />
      <FIELD NAME="SuperK" ALIAS="Super" TYPE="I4" CONDITION="1" />
      <FIELD NAME="CreateD" ALIAS="Create" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="CreateD" SIMPLENAME="~TParameterCreateD Create" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="UpdateD" ALIAS="Update" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="UpdateD" SIMPLENAME="~TParameterUpdateD Update" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="TypeC" ALIAS="Type" TYPE="I4" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.parameter.type">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10050 ORDER BY 2</LOOKUP>
      </FIELD>
      
      <FIELD NAME="ContextC" ALIAS="Context" TYPE="I4" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.parameter.context">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10052 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="AccessC" ALIAS="Access" TYPE="I4" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.parameter.access">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10053 ORDER BY 2</LOOKUP>
      </FIELD>
      
      <FIELD ID="Hashtag" NAME="ParameterK" ALIAS="Hashtag" TYPE="STR" DESCRIPTION="Hastags connected to parameter">
         <EXPRESSION>
CAST( 
STUFF( 
   (SELECT ',' + _b.FName FROM $OTBadge _b JOIN $OTrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _r.ParentK=$T.ParameterK AND _r.table_number=10040),
   1,1,'' )
   AS NVARCHAR(512)
)
         </EXPRESSION>
      </FIELD>
      <CONDITION ID="Hashtag" NAME="ParameterK" SIMPLENAME="Hashtag" TYPE="STR" NOVALUE="1" INTELLISENSE="hashtag_name">
         <EXPRESSION>
$T.$F $_operator (SELECT _r.ParentK FROM $O{}TBadge _b JOIN $O{}TrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _b.FName $_condition AND _r.table_number=10040)
         </EXPRESSION>
         <OPERATORS>
            <OPERATOR FROM="0" TO="11" />
            <OPERATOR FROM="5" TO="12" />
         </OPERATORS>
      </CONDITION>
      
      
      <FIELD NAME="FName" ALIAS="Name" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FType" ALIAS="Type" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FDescription" ALIAS="Description" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FVersionBegin" ALIAS="Version begin" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FVersionEnd" ALIAS="VersionEnd" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDeleted" ALIAS="Deleted" TYPE="I2" CONDITION="1" />
      <FIELD ID="Note" NAME="FDescription" ALIAS="Note" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _n.FNote FROM $O{}TNote _n WHERE _n.RecordK=$T.ParameterK AND _n.table_number=10040)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="ParameterK-Chapter" NAME="ParameterK" ALIAS="Chapter" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _ac.FName  FROM $O{}TArticleChapter _ac JOIN $O{}TrArticleChapterXApiParameter _x ON _ac.ArticleChapterK=_x.ArticleChapterK WHERE _x.ParameterK = $T.ParameterK)</EXPRESSION>
      </FIELD>
      <CONDITION ID="ParameterK-ArticleChapterK" NAME="ParameterK" ALIAS="ID (Chapter)" TYPE="INT">
         <EXPRESSION>(SELECT TOP 1 _ac.ArticleChapterK  FROM $O{}TArticleChapter _ac JOIN $O{}TrArticleChapterXApiParameter _x ON _ac.ArticleChapterK=_x.ArticleChapterK WHERE _x.ParameterK = $T.ParameterK)</EXPRESSION>
      </CONDITION>
      <FIELD ID="CountArticle" NAME="ParameterK" ALIAS="Count article" SIMPLENAME="Number of connected articles" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{}TrArticleXApiFunction _x WHERE _x.FunctionK=$T.$F)</EXPRESSION>
      </FIELD>
      

      <EDIT NAME="FunctionK" ALIAS="FunctionK" TYPE="I4" NULL="0" DEFAULT="0" />
      <EDIT NAME="ParameterTypeK" ALIAS="Parameter type" TYPE="I4" />
      <EDIT NAME="SuperK" ALIAS="Super" TYPE="I4" />
      <EDIT NAME="CreateD" ALIAS="Create" TYPE="DATE" />
      <EDIT NAME="UpdateD" ALIAS="Update" TYPE="DATE" />
      <EDIT NAME="TypeC" ALIAS="TypeC" TYPE="I4">
         <LOOKUP CACHE="api.parameter.type.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10050 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="ContextC" ALIAS="Context" TYPE="I4">
         <LOOKUP CACHE="api.parameter.context.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10052 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="AccessC" ALIAS="Access" TYPE="I4">
         <LOOKUP CACHE="api.parameter.access.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10053 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="FName" ALIAS="Name" TYPE="STR" MAX="250" />
      <EDIT NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" MAX="500" />
      <EDIT NAME="FType" ALIAS="Type" TYPE="STR" MAX="250" />
      <EDIT NAME="FDescription" ALIAS="Description" TYPE="STR" MAX="2000" />
      <EDIT NAME="FVersionBegin" ALIAS="VersionBegin" TYPE="I4" />
      <EDIT NAME="FVersionEnd" ALIAS="VersionEnd" TYPE="I4" />
      <EDIT NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" />
      <EDIT NAME="FDeleted" ALIAS="Deleted" TYPE="I2" />
   </TABLETEMPLATE>

  

   <TABLETEMPLATE ID="api.function">
      <FIELD NAME="RepositoryK" ALIAS="Repository" TYPE="I4" CONDITION="1" />
      <FIELD NAME="ObjectK" ALIAS="Object" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _o.FName FROM $OTObject _o WHERE _o.ObjectK=$T.ObjectK)</EXPRESSION>
      </FIELD>
      <FIELD NAME="CreateD" ALIAS="Create" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="CreateD" SIMPLENAME="~TFunctionCreateD CreateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="UpdateD" ALIAS="Update" TYPE="ODATE">
         <FORMULA CODE="GD1">date.datetimetext( float(@this) + 2 )</FORMULA>
         <EXPRESSION>CAST( [$T].[$F] AS float )</EXPRESSION>
      </FIELD>
      <CONDITION NAME="UpdateD" SIMPLENAME="~TFunctionUpdateD UpdateD" TYPE="ODATE" DEFAULT="$${date.datetimetext( date.offsethour( 0 ) )}">
         <EXPRESSION>FLOOR( CAST([$T].[$F] AS FLOAT) ) $_operator FLOOR($_value-2)</EXPRESSION>
      </CONDITION>
      <FIELD NAME="SuperK" ALIAS="Super" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT COALESCE(_f.FName,_f.FDeclaration,'') FROM $O{APP}TFunction _f WHERE _f.FunctionK=$T.SuperK)</EXPRESSION>
      </FIELD>
      <FIELD NAME="TypeC" ALIAS="Type" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.function.type">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10030 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="StateC" ALIAS="State" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.function.state">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10031 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="LanguageC" ALIAS="Language" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1" />
      <FIELD NAME="PriorityC" ALIAS="Priority" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.function.priority">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10035 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="AccessC" ALIAS="Access" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.function.access">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10036 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="ReturnC" ALIAS="return" TYPE="I4" DISPLAYTYPE="STR" CONDITION="1" DESCRIPTION="Same code as in Object">
         <EXPRESSION>(SELECT _c.FName FROM $O{CODE}TCode _c WHERE _c.CodeK=$T.$F)</EXPRESSION>
         <LOOKUP CACHE="api.object.language">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10015 ORDER BY 2</LOOKUP>
      </FIELD>
      <FIELD NAME="FName" ALIAS="Name" TYPE="STR" CONDITION="1" />
      <FIELD ID="FName-Indent" NAME="FName" ALIAS="Indented name" CONDITION="1" TYPE="STR" DESCRIPTION="Indent name based on level in function tree if it belongs to any">
         <FORMULA TYPE="HTML" />
         <EXPRESSION>
('&lt;code class="function" style="margin-left: ' + CAST( ISNULL( (SELECT TOP 1 _t.FDepth FROM $O{}thread _t WHERE _t.FKey=$T.FunctionK AND _t."table_number" = 10030), 0 ) * 2 AS VARCHAR(10)) + 'em;"&gt;' + $T.FName + '&lt;/code&gt;')
         </EXPRESSION>
      </FIELD>
      
      <FIELD NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FReturn" ALIAS="Return" TYPE="STR" CONDITION="1" />
      <FIELD ID="CountParameter" NAME="FunctionK" ALIAS="Count parameter" SIMPLENAME="Number of doccumented parameters" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTParameter _p WHERE _p.FunctionK=$T.$F)</EXPRESSION>
      </FIELD>
      <FIELD ID="CountArticle" NAME="FunctionK" ALIAS="Count article" SIMPLENAME="Number of connected articles" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $O{}TrArticleXApiFunction _x WHERE _x.FunctionK=$T.$F)</EXPRESSION>
      </FIELD>
      
      
      <FIELD ID="Overload" NAME="FDeclaration" ALIAS="Overloads" TYPE="STR" ALIGN="L" FF="v" CONDITION="expression">
         <EXPRESSION><![CDATA[
CASE WHEN (SELECT COUNT(*) FROM $OTFunctionOverload _fo WHERE _fo.FunctionK=$T.FunctionK) > 0 THEN       
ISNULL( REPLACE( $T.$F, '<', '&lt;' ) + '<br />', '') + (SELECT STRING_AGG( REPLACE( _t.FDeclaration, '<', '&lt;' ), '<br />' ) FROM (	SELECT TOP 100 _fo.FIndex, _fo.FDeclaration FROM api.TFunctionOverload _fo WHERE _fo.FunctionK=$T.FunctionK ORDER BY _fo.FIndex ) _t)
ELSE REPLACE( $T.$F, '<', '&lt;' )
END      ]]></EXPRESSION>
      </FIELD>
      <FIELD ID="Note" NAME="FDescription" ALIAS="Note" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _n.FNote FROM $O{}TNote _n WHERE _n.RecordK=$T.FunctionK AND _n.table_number=10030)</EXPRESSION>
      </FIELD>
      
      
      
      <FIELD NAME="FDescription" ALIAS="Description" TYPE="STR" CONDITION="1" />
      <FIELD NAME="FVersionBegin" ALIAS="VersionBegin" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FVersionEnd" ALIAS="VersionEnd" TYPE="I4" CONDITION="1" />
      <FIELD NAME="FBeta" ALIAS="Beta" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" CONDITION="1" />
      <FIELD NAME="FDeleted" ALIAS="Deleted" TYPE="I2" CONDITION="1" />
      
      <FIELD ID="CountExample" NAME="FunctionK" ALIAS="Count example" SIMPLENAME="Number of examples" TYPE="INT" CONDITION="expression">
         <EXPRESSION>(SELECT COUNT(*) FROM $OTExample _e WHERE _e.ParentK=$T.FunctionK AND _e.table_number=10030)</EXPRESSION>
      </FIELD>
      
      <FIELD ID="FunctionK-Chapter" NAME="FunctionK" ALIAS="Chapter" TYPE="STR" CONDITION="expression">
         <EXPRESSION>(SELECT TOP 1 _ac.FName  FROM $O{}TArticleChapter _ac JOIN $O{}TrArticleChapterXApiFunction _x ON _ac.ArticleChapterK=_x.ArticleChapterK WHERE _x.FunctionK = $T.FunctionK)</EXPRESSION>
      </FIELD>
      <CONDITION ID="FunctionK-ArticleChapterK" NAME="FunctionK" ALIAS="ID (Chapter)" TYPE="INT">
         <EXPRESSION>(SELECT TOP 1 _ac.ArticleChapterK  FROM $O{}TArticleChapter _ac JOIN $O{}TrArticleChapterXApiFunction _x ON _ac.ArticleChapterK=_x.ArticleChapterK WHERE _x.FunctionK = $T.FunctionK)</EXPRESSION>
      </CONDITION>
      
      
      <FIELD ID="Hashtag" NAME="FunctionK" ALIAS="Hashtag" TYPE="STR" DESCRIPTION="Hastags connected to function">
         <EXPRESSION>
CAST( 
STUFF( 
   (SELECT ',' + _b.FName FROM $OTBadge _b JOIN $OTrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _r.ParentK=$T.FunctionK AND _r.table_number=10010),
   1,1,'' )
   AS NVARCHAR(512)
)
         </EXPRESSION>
      </FIELD>
      <CONDITION ID="Hashtag" NAME="FunctionK" SIMPLENAME="Hashtag" TYPE="STR" NOVALUE="1" INTELLISENSE="hashtag_name">
         <EXPRESSION>
$T.$F $_operator (SELECT _r.ParentK FROM $O{}TBadge _b JOIN $O{}TrXBadge _r ON _b.BadgeK = _r.BadgeK WHERE _b.FName $_condition AND _r.table_number=10030)
         </EXPRESSION>
         <OPERATORS>
            <OPERATOR FROM="0" TO="11" />
            <OPERATOR FROM="5" TO="12" />
         </OPERATORS>
      </CONDITION>

      <EDIT NAME="RepositoryK" ALIAS="Repository" TYPE="I4" NULL="0" DEFAULT="0" />
      <EDIT NAME="ObjectK" ALIAS="Object" TYPE="I4" />
      <EDIT NAME="CreateD" ALIAS="Create" TYPE="DATE" />
      <EDIT NAME="UpdateD" ALIAS="Update" TYPE="DATE" />
      <EDIT NAME="SuperK" ALIAS="Super" TYPE="I4" />
      <EDIT NAME="TypeC" ALIAS="Type" TYPE="I4">
         <LOOKUP CACHE="api.function.type.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10030 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="StateC" ALIAS="State" TYPE="I4">
         <LOOKUP CACHE="api.function.state.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10031 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="LanguageC" ALIAS="Language" TYPE="I4">
         <LOOKUP CACHE="api.function.janguage.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10034 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="PriorityC" ALIAS="State" TYPE="I4">
         <LOOKUP CACHE="api.function.priority.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10035 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="AccessC" ALIAS="Access" TYPE="I4">
         <LOOKUP CACHE="api.function.access.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10036 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="ReturnC" ALIAS="Return" TYPE="I4" DESCRIPTION="Same as Object">
         <LOOKUP CACHE="api.object.return.edit">SELECT CodeK, FName FROM $O{CODE}TCode WHERE GroupK=10015 AND FDeleted=0 AND FIdle=0 ORDER BY 2</LOOKUP>
      </EDIT>
      <EDIT NAME="FName" ALIAS="Name" TYPE="STR" MAX="500" />
      <EDIT NAME="FDeclaration" ALIAS="Declaration" TYPE="STR" MAX="2000" />
      <EDIT NAME="FReturn" ALIAS="Return" TYPE="STR" MAX="100" />
      <EDIT NAME="FDescription" ALIAS="Description" TYPE="STR" />
      <EDIT NAME="FVersionBegin" ALIAS="Version begin" TYPE="I4" />
      <EDIT NAME="FVersionEnd" ALIAS="Version end" TYPE="I4" />
      <EDIT NAME="FBeta" ALIAS="Beta" TYPE="I2" />
      <EDIT NAME="FDepreciated" ALIAS="Depreciated" TYPE="I2" />
      <EDIT NAME="FDeleted" ALIAS="Deleted" TYPE="I2" />
   </TABLETEMPLATE>
   
